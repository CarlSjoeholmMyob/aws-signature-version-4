{
  "extends": ["github>FantasticFiasco/renovate-config"],
  "packageRules": [
    {
      "matchPackagePatterns": ["^@aws-cdk", "^aws-cdk"],
      "groupName": "AWS CDK packages"
    },
    {
      "matchPackagePatterns": ["eslint"],
      "groupName": "ESLint packages"
    }
  ],
  "ignoreDeps": ["bl", "lodash", "minimist", "netmask"]
}
